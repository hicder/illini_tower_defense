import javax.swing.ImageIcon;


public class StonyBrookCreep extends Creeps{
	public StonyBrookCreep(int x, int y){
		super(x,y);
		health = 100000;
		dx = 50;
		div = 5;
		this.x = x;
		this.y = y;
		ImageIcon ii = new ImageIcon(this.getClass().getResource("RiS_BlueBullet.png"));
		creepImage = ii.getImage();
	}

}
