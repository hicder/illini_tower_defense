import javax.swing.ImageIcon;


public class UnionTower extends Towers{
	// Standard Class. Shots normal speed does normal damage.

	public UnionTower(){
		name= "Union";
		damage = 100;
		range = 175;
		fireRate = 3;
		level=1;
		ImageIcon ii = new ImageIcon(this.getClass().getResource("TowerUnion.png"));
		towerImage = ii.getImage();
	}
	
	public UnionTower(Towers a){
		super(a);
	}
	
}
